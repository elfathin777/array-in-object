
public class ArrayMatrices {
	public static void main(String[] args) {
		int a [][] = {{1, 3, 4}, {3, 4, 5}};
		int b [][] = {{1, 3, 4}, {3, 4, 5}};
		int n [][] = new int [a][b];
		
		for (int i = 0; i < 2; i++) {
			for (int j = 0; j < 3; j++) {
				n[i][j] = a[i][j] + b[i][j];
				System.out.print(n[i][j]);
				
			}
		}
	}
}